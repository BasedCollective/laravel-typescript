{
    "name": "basedcollective/laravel-typescript",
    "description": "Transform Laravel models into TypeScript interfaces",
    "keywords": [
        "laravel",
        "typescript"
    ],
    "homepage": "https://github.com/BasedCollective/laravel-typescript",
    "license": "MIT",
    "authors": [
        {
            "name": "Ostap Brehin",
            "email": "osbre@protonmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0 || ^11.0 || ^12.0",
        "illuminate/database": "^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0",
        "spatie/laravel-package-tools": "^1.11.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.0 || ^3.0",
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Based\\TypeScript\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Based\\TypeScript\\Tests\\": "tests"
        }
    },
    "scripts": {
        "stan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Based\\TypeScript\\TypeScriptServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
